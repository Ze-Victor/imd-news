{
  "expo": {
    "name": "IMD News",
    "slug": "imdnews",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#212121"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dispositivosmoveis.imdnews"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#212121"
      },
      "package": "com.dispositivosmoveis.imdnews",
      "version": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}